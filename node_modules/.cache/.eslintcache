y === \"community\") {\n      setCommunity(dataFilter.value);\n    }\n  };\n\n  const handleReset = () => {\n    setBreed(\"all\");\n    setCommunity(\"all\");\n  };\n\n  const filterDogs = dogs\n    .filter((dogs) => {\n      return breeds === \"all\" ? true : dogs.breeds.includes(breeds);\n    })\n    .filter((dogs) => {\n      return community === \"all\" ? true : dogs.community === community;\n    });\n\n  const renderLanding = (props) => {\n    return (\n      <>\n        <header className=\"header\">\n          <div className=\"logo\">\n            <img className=\"header__logo\" src={logo} alt=\"logo waggin\" />\n          </div>\n          <div className=\"header__others\">\n            <Link className=\"link__search\" to=\"/\">\n              <i className=\"header__others--search fas fa-search\"></i>\n            </Link>\n            <Link className=\"link__search\" to=\"/Favs\">\n              <i className=\"header__others--bone fas fa-bone\"></i>\n            </Link>\n          </div>\n        </header>\n        <header className=\"header2\">\n          <div className=\"logowide\">\n            <img className=\"header2__logo\" src={logowide} alt=\"logo waggin\" />\n          </div>\n          <div className=\"header2__others\">\n            <Link className=\"link__search\" to=\"/\">\n              <i className=\"header2__others--search fas fa-search\"></i>\n            </Link>\n            <Link className=\"link__search\" to=\"/Favs\">\n              <i className=\"header2__others--bone fas fa-bone\"></i>\n            </Link>\n          </div>\n        </header>\n        <section className=\"welcome\">Adopta a tu mascota ideal con Waggin</section>\n        <section className=\"main__filters\">\n          <Filter\n            className=\"filters\"\n            handleFilter={handleFilter}\n            handleReset={handleReset}\n            breed={breeds}\n            community={community}\n          />\n          <Link className=\"link__search\" to=\"/dog/\">\n            <p className=\"link__search--button\"> Buscar </p>\n          </Link>\n        </section>\n      </>\n    );\n  };\n\n  const renderNone = (props) => {\n    return <None />;\n  };\n\n  const renderFavs = () => {\n    return <Favs />;\n  };\n\n  const renderSearch = (props) => {\n    return <DogList dogs={filterDogs} />;\n  };\n\n  const renderDogCard = (props) => {\n    const id = parseInt(props.match.params.id);\n    const dogCard = dogs.find((dog) => {\n      return dog.id === id;\n    });\n    return <DogCard dogCard={dogCard} />;\n  };\n\n  return (\n    <Switch>\n      <Route exact path=\"/\" component={renderLanding} />\n      <Route path=\"/dog/:id\" render={renderDogCard} />\n      <Route path=\"/dog\" render={renderSearch} />\n      <Route path=\"/None\" render={renderNone} />\n      <Route path=\"/Favs\" render={renderFavs} />\n    </Switch>\n  );\n};\n\nexport default App;\n","/mnt/c/Users/marta/OneDrive/Escritorio/adalab/waggin/src/components/None.js",["54"],"import React, { useState } from \"react\";\nimport \"../stylesheets/layouts/None.scss\";\nimport { Link } from \"react-router-dom\";\nimport logo from \"../images/logo-w.png\";\nimport logowide from \"../images/logo.png\";\n\nconst None = (props) => {\n  return (\n    <>\n      <header className=\"header\">\n        <div className=\"logo\">\n          <img className=\"header__logo\" src={logo} alt=\"logo waggin\" />\n        </div>\n        <div className=\"header__others\">\n          <Link className=\"link__search\" to=\"/\">\n            <i className=\"header__dogList--search fas fa-search\"></i>\n          </Link>\n          <Link className=\"link__search\" to=\"/Favs\">\n            <i className=\"header__dogList--bone fas fa-bone\"></i>\n          </Link>\n        </div>\n      </header>\n      <header className=\"header2\">\n          <div className=\"logowide\">\n            <img className=\"header2__logo\" src={logowide} alt=\"logo waggin\" />\n          </div>\n          <d